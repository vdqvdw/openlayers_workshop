{
  "gitbook": "^3.2.3",
  "variables": {
    "workshopUrl": "http://localhost:3000"
  },
  "links": {
    "sidebar": {
      "OpenLayers Workshop": "/"
    }
  },
  "plugins": [
    "image-captions",
    "include",
    "include-codeblock-patched",
    "copy-code-button"
  ],
  "pluginsConfig": {
    "image-captions": {
      "caption": "_CAPTION_"
    },
    "include-codeblock": {
      "unindent": true
    }
  },
  "styles": {
    "website": "style.css"
  }
}
